set realname = ""
set from = "@gmail.com"
set use_from = yes
set envelope_from = yes

set smtp_url = "smtps://@gmail.com@smtp.gmail.com:587/"
set smtp_pass = ""
set imap_user = "@gmail.com"
set imap_pass = ""
set folder = "imaps://imap.gmail.com:993"
set spoolfile = "+INBOX"
set ssl_force_tls = yes

# G to get mail
bind index G imap-fetch-mail
set editor = "vim"
set charset = "utf-8"
set record = ''
